<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.User" table="ji_user">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="nick" type="java.lang.String">
            <column name="nick" length="20" not-null="true" />
        </property>
        <property name="phone" type="java.lang.Integer">
            <column name="phone" length="11" not-null="true" />
        </property>
        <property name="password" type="java.lang.String">
            <column name="password" length="20" not-null="true" />
        </property>
      	<property name="signature" type="java.lang.String">
            <column name="signature" length="20" not-null="true" />
        </property>
             <property name="age" type="java.lang.Integer">
            <column name="age" length="2" not-null="true" />
        </property>
        <property name="gender" type="java.lang.Integer">
            <column name="gender" length="1" not-null="true" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" length="20" not-null="true" />
        </property>
        <property name="create_time" type="java.lang.Integer">
            <column name="create_time" length="200" not-null="true" />
        </property>
       <property name="score" type="java.lang.Integer">
            <column name="score" length="20" not-null="true" />
        </property>
        <property name="money" type="java.lang.String">
            <column name="money" length="20" not-null="true" />
        </property>
      	<property name="level" type="java.lang.Integer">
            <column name="level" length="2" not-null="true" />
        </property>
        <property name="certification" type="java.lang.Integer">
            <column name="certification" length="1" not-null="true" />
        </property>
          <property name="status" type="java.lang.Integer">
            <column name="status" length="2" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
