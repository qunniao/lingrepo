<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Product" table="ji_product">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="store_id" type="java.lang.String">
            <column name="store_id" length="20" not-null="true" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="20" not-null="true" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" length="20" not-null="true" />
        </property>
      	<property name="note" type="java.lang.String">
            <column name="note" length="20" not-null="true" />
        </property>
             <property name="old_price" type="java.lang.String">
            <column name="old_price" length="20" not-null="true" />
        </property>
        <property name="new_price" type="java.lang.String">
            <column name="new_price" length="100" not-null="true" />
        </property>
        <property name="freight" type="java.lang.String">
            <column name="freight" length="20" not-null="true" />
        </property>
        <property name="seven_day" type="java.lang.String">
            <column name="seven_day" length="200" not-null="true" />
        </property>
       <property name="cash" type="java.lang.String">
            <column name="cash" length="20" not-null="true" />
        </property>
        <property name="sales" type="java.lang.String">
            <column name="sales" length="20" not-null="true" />
        </property>
      	<property name="pic" type="java.lang.String">
            <column name="pic" length="20" not-null="true" />
        </property>
        <property name="details" type="java.lang.String">
            <column name="details" length="1" not-null="true" />
        </property>
          <property name="status" type="java.lang.String">
            <column name="status" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>

